{
  "name": "foo-service merge",
  "repository": "git@github.com:adikari/buildkite-docker-example.git",
  "default_branch": "master",
  "steps": [
    {
      "type": "script",
      "name": ":buildkite: Merge",
      "command": "buildkite-agent pipeline upload foo-service/.buildkite/merge.yml"
    }
  ],
  "description": "Pipeline for foo service merge",
  "cancel_running_branch_builds": false,
  "skip_queued_branch_builds": false,
  "branch_configuration": "master",
  "provider_settings": {
    "trigger_mode": "none"
  }
}
