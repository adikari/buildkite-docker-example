{
  "name": "foo service pull-request",
  "repository": "git@github.com:adikari/buildkite-docker-example.git",
  "default_branch": "",
  "steps": [
    {
      "type": "script",
      "name": ":buildkite: Foo Service Pull Request",
      "command": "buildkite-agent pipeline upload foo-service/.buildkite/pull-request.yml"
    }
  ],
  "description": "Pipeline for foo service pull request",
  "cancel_running_branch_builds": true,
  "skip_queued_branch_builds": true,
  "branch_configuration": "!master",
  "provider_settings": {
    "trigger_mode": "none"
  }
}
