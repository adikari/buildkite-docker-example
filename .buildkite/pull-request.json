{
  "name": "pull-request",
  "repository": "git@github.com:adikari/buildkite-docker-example.git",
  "default_branch": "",
  "steps": [
    {
      "type": "script",
      "name": ":buildkite: Pull Request",
      "command": "bulidkite-agent pipeline upload .buildkite/pull-request.yml"
    }
  ],
  "description": "Pipeline for all pull requests",
  "cancel_running_branch_builds": true,
  "skip_queued_branch_builds": true,
  "branch_configuration": "!master",
  "provider_settings": {
    "pull_request_branch_filter_enabled": true,
    "pull_request_branch_filter_configuration": "!master",
    "trigger_mode": "code",
    "publish_commit_status_per_step": true,
    "publish_blocked_as_pending": "true",
    "separate_pull_request_statuses": true
  }
}
